.travisSection {
   background-image url('../img/general/travis_bg.jpg')
   background-position bottom
   @media $md-lg {
      height 550px
   }
   @media $sm {
      height 520px
   }





   .textContainer {
      align-self flex-start
      margin $space-4 0 0 20%
      @media $md-lg {
         width 400px
      }
      @media $sm {
         max-width 360px
         margin-left 35%
      }
      @media (max-width 720px) {
         margin-left 30%
      }
      @media (max-width: 590px) {
         margin-left 20%
      }
      @media (max-width: 510px) {
         margin-left 10%
      }
      @media (max-width: 460px) {
         margin-left 5%
      }
      @media (max-width: 430px) {
         margin-left 0
      }
   }
   .textContainer h2 {
      color $color-black2
   }
   .textContainer h2:first-child {
      @media $md-lg {
         font-size $font-7
      }
      @media $sm {
         font-size $font-7 * .9
      }
   }
   .textContainer h2:last-child {
      align-self: flex-end
      margin-top $space-3
      @media $lg {
         width 285px
      }
      @media $md {
         width 250px
      }
      @media $sm {
         max-width 230px
      }
   }

}
