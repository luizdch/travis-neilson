.vineSection {
   background-image url('../img/general/vine_bg.jpg')
   @media $md-lg {
      height 550px
   }





   .vineCardEmbedContainer {
      width 100%
      margin-top 0
   }
   .vineCardEmbed {
      preserveAspectRatio(1, 1)
      overflow hidden
      border-radius 10px
      @media $lg {
         width 400px
      }
      @media $sm-md {
         max-width 300px
      }
   }
   .vineCardEmbed iframe {
      fullBox()
   }





   .standardButton {
      color $color-green1
   }

}
